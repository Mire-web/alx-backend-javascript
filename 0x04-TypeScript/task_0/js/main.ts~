interface Student {
  firstName: string;
  lastName: string;
  age: number;
  location: string;
}

const student1: Student = {
  firstName: Blacq;
  lastName: Nwangele;
  age: 21;
  location: Ikwo;
}

const student2: Student = {
  firstName: Mirey;
  lastName: Dev;
  age: 23;
  location: Ikwo;
}

const studentsList:Student[] = [student1, student2];